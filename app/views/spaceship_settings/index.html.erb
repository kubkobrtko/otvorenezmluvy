<div style="clear: both;"></div>
<div id="content">
  <% @settings.each do |setting| %>
      <%= form_for setting, :url => spaceship_setting_path(setting) do |f| %>
          <p>
            <%= setting.identifier  %>
          </p>
          <p>
            <%= f.label :label %>:
            <%= f.text_field :label %>
          </p>

          <p>
            <%= f.label :value %>:
            <%= f.text_field :value %>
          </p>
          <%= f.submit 'Narvi to tam' %>
      <% end %>
      <hr/>
  <% end %>
</div>